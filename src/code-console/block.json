{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "triskelion/code-console",
	"version": "1.0.0",
	"title": "Code Console",
	"category": "widgets",
	"icon": "terminal",
	"description": "Consola de código con pestañas y tracking GTM.",
	"attributes": {
		"tabs": {
			"type": "array",
			"default": [
				{
					"title": "index.js",
					"language": "javascript",
					"content": ""
				}
			]
		},
		"activeTabIndex": {
			"type": "number",
			"default": 0
		},
		"anchor": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"html": false
	},
	"textdomain": "code-console",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
